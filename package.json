{
  "name": "dog-counter-2.0",
  "version": "0.1.0",
  "description": "An electron version of the Dog Counter app. (originally written by Jason Arvin)",
  "repository": "https://github.com/jeffinnes/electron-dog-counter",
  "main": "src/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "author": "Jeff Innes",
  "license": "ISC",
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "4.0.0"
  },
  "dependencies": {
    "electron-compile": "^6.4.4",
    "electron-squirrel-startup": "^1.0.0"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "dmg",
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "icon": "icons/png/64x64"
      },
      "electronWinstallerConfig": {
        "name": "Dog Counter 2.0",
        "icon": "icons/dogecoin",
        "setupIcon": "icons/dogecoin",
        "setupMsi": "setupDoge"
      },
      "electronInstallerDMG": {
        "name": "Dog Counter 2.0",
        "title": "Dog Counter 2.0",
        "icon": "icons/dogecoin"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "dogcounter2.0"
      }
    }
  }
}
